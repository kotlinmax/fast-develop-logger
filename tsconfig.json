{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ESNext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {},
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*", "src/bases"],
  "exclude": ["node_modules", "dist", "prisma"]
}
